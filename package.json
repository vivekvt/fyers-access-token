{
  "name": "fyers-access-token",
  "version": "1.0.0",
  "description": "Generate Fyers Access Token with TOTP login using Fyers API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vivekvt/fyers-access-token.git"
  },
  "homepage": "https://github.com/vivekvt/fyers-access-token#readme",
  "bugs": {
    "url": "https://github.com/vivekvt/fyers-access-token/issues"
  },
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "lint": "eslint --ignore-path .eslintignore --fix --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "prepare": "husky install"
  },
  "author": "vivekvt",
  "license": "MIT",
  "keywords": [
    "fyers",
    "access token",
    "fyers API",
    "fyers totp"
  ],
  "dependencies": {
    "axios": "^1.3.4",
    "query-string": "^7.1.3",
    "totp-generator": "^0.0.14"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@types/totp-generator": "^0.0.5",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/**/*"
  ]
}
